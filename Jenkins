pipeline {
    agent any

    tools {
        maven 'maven'
    }

    stages {
        stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }
        stage('Test') {
            steps {
                echo 'Test'
            }
        }
        stage('Code Quality') {
            steps {
                echo 'Code Quality'
            }
        }
        stage ('Deploy to Dev') {
            steps { 
                echo 'Dev'
            }


        }
        stage ('Deploy to Test') {
            steps {
                echo UAT
            }

        }
    }
}